<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="baseAnchorPane" prefHeight="442.0" prefWidth="628.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="customerController">
   <children>
      <TableView fx:id="tableView" layoutX="22.0" layoutY="52.0" prefHeight="271.0" prefWidth="588.0">
        <columns>
          <TableColumn fx:id="custId" prefWidth="75.0" text="CustomerID" />
          <TableColumn fx:id="custName" prefWidth="332.0" text="Name" />
            <TableColumn fx:id="custHP" prefWidth="96.0" text="Phone Number" />
            <TableColumn fx:id="custPCode" prefWidth="84.0" text="Postcode" />
        </columns>
      </TableView>
      <Button fx:id="chooseFileButton" layoutX="188.0" layoutY="357.0" mnemonicParsing="false" onAction="#chooseFileButtonPressed" text="Choose a file" />
      <Text layoutX="22.0" layoutY="31.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Import Customer Data">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <Button fx:id="importButton" layoutX="344.0" layoutY="357.0" mnemonicParsing="false" onAction="#importFileButtonPressed" text="Import File" />
      <Button fx:id="backButton" layoutX="22.0" layoutY="357.0" mnemonicParsing="false" onAction="#backButtonPressed" text="Back" />
   </children>
</AnchorPane>
