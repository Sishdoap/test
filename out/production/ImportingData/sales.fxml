<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane fx:id="baseAnchorPane" prefHeight="442.0" prefWidth="628.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="salesController">
   <children>
      <Text layoutX="26.0" layoutY="38.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Import Sales Data">
         <font>
            <Font size="18.0" />
         </font>
      </Text>
      <TableView fx:id="tableView" layoutX="26.0" layoutY="57.0" prefHeight="274.0" prefWidth="578.0">
        <columns>
          <TableColumn fx:id="salesID" prefWidth="84.0" text="salesID" />
          <TableColumn fx:id="date" prefWidth="164.0" text="Date" />
            <TableColumn fx:id="carPlate" prefWidth="129.0" text="Car Plate" />
            <TableColumn fx:id="customerId" prefWidth="99.0" text="customerId" />
            <TableColumn fx:id="employeeID" prefWidth="101.0" text="employeeId" />
        </columns>
      </TableView>
      <Button fx:id="chooseFileButton" layoutX="169.0" layoutY="363.0" mnemonicParsing="false" onAction="#chooseFileButtonPressed" text="Choose File" />
      <Button fx:id="importButton" layoutX="350.0" layoutY="363.0" mnemonicParsing="false" onAction="#importFileButtonPressed" text="Import File" />
      <Button fx:id="backButton" layoutX="26.0" layoutY="363.0" mnemonicParsing="false" onAction="#backButtonPressed" text="Back" />
   </children>
</AnchorPane>
